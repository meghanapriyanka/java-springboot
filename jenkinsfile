pipeline{
    agent any

    stages{
        stage('Build'){
            steps{
                echo 'Build'
            }

        }

        stage('Test') {
            steps{
                echo 'Test'
            }

        }
        stage('Sonarqube'){
            steps{
                echo 'Sonarcube'
            }

        }
        stage('Push to artifactory'){
            steps{
                echo 'Push to artifactory'
            }
        }
        stage ('Deploy to QA')
        {
            steps
            {
                echo 'Deploy to QA'
            }
        }
        stage('Deploy to prod') {
            steps
            {
                echo 'Deploy to prod'
            }
        }

    }
    post
    {
        always 
        {
            echo 'I will always say hello again!'
        }
    }

}